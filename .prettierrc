    "tabWidth": 2, 
    "useTabs": false, 
    "singleQuote": true,
    "semi": true, 
    "trailingComma": "none", 
    "bracketSpacing": true, 
    "parser": "babylon",
    "overrides": [
        {
          "files": "*.test.js",
          "options": {
            "semi": true
          }
        },
        {
          "files": ["*.html", "legacy/**/*.js"],
